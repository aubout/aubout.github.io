---
layout: default
---
<article class="post">
  {% if page.category == origtext %} 
  <link rel="stylesheet" href="{{ "/assets/origtext.css" | relative_url }}">
  {% endif %}
  <header class="post-header">
    <h1 class="post-title">{{ page.title | escape }}</h1>
  </header>
    
  <div class="post-content">
    {{ content }}
  </div>

</article>
